<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.vmfld.gonggumi">

    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="공그미"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity
            android:name=".Activity.SplashActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Activity.MainListActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Activity.MakeRoomActivity"
            android:label="@string/title_activity_make_room"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Activity.MakeRoomPopActivity"
            android:theme="@style/AppTheme.Pop" />
        <activity
            android:name=".Activity.SearchViewActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Activity.SearchRoomPopActivity"
            android:theme="@style/AppTheme.Pop" />
        <activity
            android:name=".Activity.RoomActivity"
            android:label="@string/title_activity_room"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Activity.CodeGetActivity"
            android:theme="@style/AppTheme.Pop" />
        <activity
            android:name=".Activity.ManageMoneyActivity"
            android:label="@string/title_activity_manage_money"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Activity.HistoryActivity"
            android:label="@string/title_activity_history"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".Activity.DivideActivity"></activity>
    </application>

</manifest>